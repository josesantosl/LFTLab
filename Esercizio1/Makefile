	java TreZeri "1100011001"
	@echo "=== INVALID TESTS ==="
	java TreZeri "10214"
12:
	javac indentificatore.java
	@echo "==== VALID TESTS ===="
	java indentificatore "x"
	java indentificatore "flag1"
	java indentificatore "x2y2"
	java indentificatore "x_1"
	java indentificatore "lft_lab"
	java indentificatore "_temp"
	java indentificatore "x_1_y_2"
	java indentificatore "x___"
	java indentificatore "__5"
	@echo "=== INVALID TESTS ==="
	java indentificatore "5"
	java indentificatore "221B"
	java indentificatore "123"
	java indentificatore "9_to_5"
	java indentificatore "___"
