110:
	javac Es110.java
	@echo "==== VALID TESTS ===="
	java Es110 "aaa/****/aa"
	java Es110 "aa/*a*a*/"
	java Es110 "aaaaa"
	java Es110 "/*****/"
	java Es110 "/*aa*/"
	java Es110 "*/a"
	java Es110 "a/**/***a"
	java Es110 "a/**/***/a"
	java Es110 "a/**/aa/***/a"
	@echo "=== INVALID TESTS ==="
	java Es110 "aaa/*/aa"
	java Es110 "a/**//***a"
	java Es110 "aa/*aa"
